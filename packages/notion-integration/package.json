{
  "name": "@researchflow/notion-integration",
  "version": "1.0.0",
  "description": "Notion API integration for ResearchFlow deployment tracking",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
